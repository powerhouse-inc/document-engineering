      "types": "./dist/src/scalars/index.d.ts",
      "import": "./dist/src/scalars/index.js"
    },
    "./scalars/*": {
      "types": "./dist/src/scalars/*",
      "import": "./dist/src/scalars/*"
    },
    "./ui": {
      "types": "./dist/src/ui/index.d.ts",
      "import": "./dist/src/ui/index.js"
    },
