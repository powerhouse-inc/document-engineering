      "types": "./dist/src/scalars/graphql/scalars.d.ts",
      "import": "./dist/src/scalars/graphql/scalars.js"
    },
    "./graphql-scalars/*": {
      "types": "./dist/src/scalars/graphql/*",
      "import": "./dist/src/scalars/graphql/*"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "tsc": "tsc",
    "tailwind": "npx @tailwindcss/cli -i style.css -o dist/style.css",
    "copy-assets": "mkdir -p dist/src/assets && cp -r src/assets dist/src",
    "build": "pnpm tsc && pnpm tailwind && pnpm copy-assets",
    "lint": "eslint --config eslint.config.js --cache --cache-strategy content",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepare": "husky",
    "release": "semantic-release"
